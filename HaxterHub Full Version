local ReplicatedStorage = game:GetService("ReplicatedStorage")
local remote = ReplicatedStorage:WaitForChild("BloxbizRemotes"):WaitForChild("CatalogOnApplyToRealHumanoid")

local assetIds = {
	3398308134,
	3033910400, 
	617605556, 
	4324158403, 
	3822880197, 
	3443038622, 
	4154538250, 
