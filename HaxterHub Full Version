    local lettersss = {
        ["A"] = "А", ["a"] = "ล",
        ["B"] = "B", ["b"] = "b",
        ["C"] = "C", ["c"] = "ᲃ",
        ["D"] = "D", ["d"] = "d",
        ["E"] = "Е", ["e"] = "e",
        ["F"] = "ꜰ", ["f"] = "f",
        ["G"] = "G", ["g"] = "g",
        ["H"] = "H", ["h"] = "һ",
        ["I"] = "Ι", ["i"] = "i〪",
        ["J"] = "J", ["j"] = "ј",
        ["K"] = "K", ["k"] = "k",
        ["L"] = "L", ["l"] = "ӏ",
        ["M"] = "M", ["m"] = "m",
        ["N"] = "N", ["n"] = "ⴖ",
        ["O"] = "O", ["o"] = "o",
        ["P"] = "Р", ["p"] = "p",
        ["Q"] = "Q", ["q"] = "q",
        ["R"] = "R", ["r"] = "r",
        ["S"] = "S", ["s"] = "ร",
        ["T"] = "T", ["t"] = "t",
        ["U"] = "U", ["u"] = "u〪",
        ["V"] = "V", ["v"] = "v",
        ["W"] = "W", ["w"] = "w",
        ["X"] = "X", ["x"] = "x",
        ["Y"] = "Y", ["y"] = "y",
        ["Z"] = "Z", ["z"] = "z",
        [" "] = "",
        }
    
function transformName(input)
    local transformed = ""
    for i = 1, #input do
        local char = input:sub(i, i) 
        transformed = transformed .. (lettersss[char] or char) 
    end
    return transformed
end
main:CreateBox("Reason", "string", function(AutoReason)
    AutoReasonss = transformName(AutoReason)
end)

