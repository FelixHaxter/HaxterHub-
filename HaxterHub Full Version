local prefixCharacter = " "  -- **CHANGE THIS to your desired prefix character**
local suffixCharacter = " "  -- **CHANGE THIS to your desired suffix character**

local tcs = game:GetService("TextChatService")
local chats = game:GetService("Chat")
local chat = tcs.ChatInputBarConfiguration.TargetTextChannel
local ReplicatedStorage = game:GetService("ReplicatedStorage")


local letters = {
    ["A"] = "А", ["a"] = "ล",
    ["B"] = "B", ["b"] = "b",
    ["C"] = "C", ["c"] = "ᲃ",
    ["D"] = "D", ["d"] = "d",
    ["E"] = "Е", ["e"] = "e",
    ["F"] = "ꜰ", ["f"] = "f",
    ["G"] = "G", ["g"] = "g",
    ["H"] = "H", ["h"] = "һ",
    ["I"] = "Ι", ["i"] = "i〪",
    ["J"] = "J", ["j"] = "ј",
    ["K"] = "K", ["k"] = "k",
    ["L"] = "L", ["l"] = "ӏ",
    ["M"] = "M", ["m"] = "m",
    ["N"] = "N", ["n"] = "ⴖ",
    ["O"] = "O", ["o"] = "o",
    ["P"] = "Р", ["p"] = "p",
    ["Q"] = "Q", ["q"] = "q",
    ["R"] = "R", ["r"] = "r",
    ["S"] = "S", ["s"] = "ร",
    ["T"] = "T", ["t"] = "t",
    ["U"] = "U", ["u"] = "u〪",
    ["V"] = "V", ["v"] = "v",
    ["W"] = "W", ["w"] = "w",
    ["X"] = "X", ["x"] = "x",
    ["Y"] = "Y", ["y"] = "y",
    ["Z"] = "Z", ["z"] = "z",
    [" "] = "",
}



function replace(str, find_str, replace_str)
    local escaped_find_str = find_str:gsub("[%-%^%$%(%)%%%.%[%]%*%+%-%?]", "%%%0")
    return str:gsub(escaped_find_str, replace_str)
end

function filter(message, tableToUse, prefix, suffix)
    local convertedMessage = message
    for letter, replacement in pairs(tableToUse) do
        convertedMessage = replace(convertedMessage, letter, replacement)
    end
    return prefix .. convertedMessage .. suffix
end

function clearlogsf()
    for i = 1, 19 do
        chat:SendAsync("")
    end
end

function showNotification(title, message)
end

function sendChat(msg)
    local conversionTable = letters
    local converted = filter(msg, conversionTable, prefixCharacter, suffixCharacter)
    local filteredMessage = chats:FilterStringForBroadcast(converted, game.Players.LocalPlayer)

    if filteredMessage ~= converted then
        showNotification("Message Tagged", "Message filtered using letters")
    end

    if tcs.ChatVersion == Enum.ChatVersion.LegacyChatService then
        ReplicatedStorage:FindFirstChild("DefaultChatSystemChatEvents").SayMessageRequest:FireServer(converted, "All")
    else
        chat:SendAsync(converted)
    end
end
Universal:CreateBox("Chat Input", "string", function(boxs)
    sendChat(boxs)
end)

Universal:CreateButton("clear logs", function(OPENS) 
    clearlogsf()
end)


function saveAdmin(userId, username)
    if Admins[userId] then
        Message("Error", username .. " is already an admin.", 5)
        return 
    end

    local success, content = pcall(function()
        return readfile(adminFilePath)
    end)

    if not success then
        content = ""
    end

    local newLine = string.format("%s,%s", userId, username)
    content = content .. newLine .. "\n"
    writefile(adminFilePath, content) 

    Admins[userId] = username 
end


function isAdmin(playerId)
    return Admins[playerId] ~= nil
end


Target:CreateButton("Give Player Admin", function()
    if Targetnms and Targetnms ~= "" then
        local TargetPlayer = GetPlayer(Targetnms)
        if TargetPlayer then 
            local userId = TargetPlayer.UserId
            local username = TargetPlayer.Name
            
            
            if Admins[userId] then
                Message("Error", username .. " already has admin rights.", 5)
            else
                saveAdmin(userId, username) 
                Message("Admin Granted", username .. " now has admin.", 5) 
            end
        else
            Message("Error Occurred", "Username Invalid", 5)
        end
    else
        Message("Error Occurred", "No username provided", 5)
    end
end)


Target:CreateButton("Revoke Admin", function()
    if Targetnms and Targetnms ~= "" then
        local TargetPlayer = GetPlayer(Targetnms)
        if TargetPlayer and Admins[TargetPlayer.UserId] then
            Admins[TargetPlayer.UserId] = nil 
            
            Message("Admin Revoked", TargetPlayer.Name .. " no longer has admin.", 5) 
        else
            Message("Error Occurred", "Username Invalid or player does not have admin", 5)
        end
    else
        Message("Error Occurred", "No username provided", 5)
    end
end)



