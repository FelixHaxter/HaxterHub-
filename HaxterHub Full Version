          game:GetService("Workspace").Map.block:Destroy()
          game:GetService("Workspace").Map.VipDoor:Destroy()
  end)
  local players = game.Players
  local player = plr
  local character = player.Character
      local hum = character:WaitForChild("Humanoid")
      local hrp = character:WaitForChild("HumanoidRootPart")
      local respawnCFrame 
      local respawnEnabled = false 
      function GetCharacter(Player)
        if plr.Character then
            return plr.Character
        end
    end
      function GetRoot(Player)
        if GetCharacter(Player):FindFirstChild("HumanoidRootPart") then
            return GetCharacter(Player).HumanoidRootPart
        end
    end


      function onDeath(character)
          if character:FindFirstChild("HumanoidRootPart") then
              
              respawnCFrame = character.HumanoidRootPart.CFrame
          end
      
          
          character:BreakJoints()
      end
      
      
      function onRespawn(character)
          
          local humanoid = character:WaitForChild("Humanoid")
          
          
          humanoid.Health = humanoid.MaxHealth
      
          
          wait(0.1)
      
          
          if respawnEnabled and respawnCFrame then
              character:SetPrimaryPartCFrame(respawnCFrame)
          end
      end
      
      
      function setupCharacter(character)
          local humanoid = character:WaitForChild("Humanoid")
      
          
          humanoid.Died:Connect(function()
              onDeath(character)
          end)
      
          
          humanoid.HealthChanged:Connect(function(health)
              if health <= 0 then
                  
                  wait(0) 
                  player.CharacterAdded:Wait(0) 
                  onRespawn(player.Character)
              end
          end)
      end
function GetPos()
    return plr.Character.HumanoidRootPart.CFrame
end
